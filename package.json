{
  "name": "feishu-bitable-approval-print",
  "version": "1.0.0",
  "description": "审批打印插件 - 支持独立浏览器使用的审批记录管理和打印工具",
  "main": "src/index.tsx",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "dev": "npx webpack serve --mode development",
    "build:prod": "NODE_ENV=production npx webpack --mode production",
    "build:dev": "NODE_ENV=development npx webpack --mode development",
    "deploy": "npm run build:prod && gh-pages -d dist",
    "deploy:preview": "npm run build:prod && npx http-server dist -p 3003 -o",
    "clean": "rm -rf dist",
    "env:check": "node -e \"console.log('环境:', process.env.NODE_ENV || 'development'); console.log('开发模式:', process.env.REACT_APP_DEV_MODE || 'true');\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.80.0",
    "@types/react-grid-layout": "^1.3.5",
    "antd": "^5.12.8",
    "dayjs": "^1.11.10",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.5.2",
    "react-resizable": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "typescript": "^4.9.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^4.15.1"
  },
  "homepage": "https://ivanli163.github.io/feishu-approval-print"
